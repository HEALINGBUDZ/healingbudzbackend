{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "aws/aws-sdk-php": "~3.0",
        "berkayk/onesignal-laravel": "^0.9.7",
        "christofferok/laravel-emojione": "^2.1",
        "intervention/image": "^2.4",
        "jenssegers/agent": "^2.6",
        "jorenvanhocht/laravel-share": "^1.0",
        "laravel/cashier": "~7.0",
        "laravel/framework": "5.4.*",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "maddhatter/laravel-fullcalendar": "~1.0",
        "mnshankar/CSV": "^1.8",
        "pda/pheanstalk": "~3.0",
        "predis/predis": "~1.0",
        "sunra/php-simple-html-dom-parser": "1.5.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~7.5"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
          "app/PublicFunctions/CommanFunctions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Traits\\":"app/Traits",
            "Requests\\":"app/Http/Requests",
            "Repositories\\":"app/Repositories",
            "RepoFactories\\":"app/Repositories/Factories",
            "RepoInterfaces\\":"app/Repositories/Interfaces"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
